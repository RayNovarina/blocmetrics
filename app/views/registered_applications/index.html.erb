<header class="css-body-header">
  <%= render partial: 'index_page_header',
             locals: @view.locals = { title: 'My Monitored Sites' }
  %>
</header>

<main class="css-body-main">
  <% @view.apps.each_with_index do |app, index| %>
    <%= render partial: 'registered_app',
               locals: @view.locals = { app: app, apps_index: index }
    %>
  <% end %>
</main>
